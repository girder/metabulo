{
  "name": "viime",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:pug": "pug-lint-vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit"
  },
  "dependencies": {
    "@girder/components": "v3.0.0-alpha.3",
    "@mdi/font": "^3.5.95",
    "@openfonts/barlow-condensed_all": "^0.1.1",
    "@sentry/browser": "^5.0.7",
    "@sentry/integrations": "^5.0.7",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-color": "^1.2.2",
    "@types/d3-drag": "^1.2.3",
    "@types/d3-force": "^1.2.1",
    "@types/d3-format": "^1.3.1",
    "@types/d3-hierarchy": "^1.1.6",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.2",
    "@types/d3-transition": "^1.1.5",
    "@types/d3-zoom": "^1.7.4",
    "@types/papaparse": "^5.0.1",
    "axios": "^0.18.0",
    "c3": "^0.7.8",
    "core-js": "^3.3.2",
    "d3-array": "^2.3.1",
    "d3-axis": "^1.0.12",
    "d3-boxplot": "^0.7.5",
    "d3-color": "^1.4.0",
    "d3-drag": "^1.2.4",
    "d3-force": "^2.0.1",
    "d3-format": "^1.3.2",
    "d3-hierarchy": "^1.1.8",
    "d3-scale": "^2.2.2",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.5",
    "d3-transition": "^1.2.0",
    "d3-zoom": "^1.8.3",
    "lodash": "^4.17.13",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^4.0.5",
    "papaparse": "^4.6.3",
    "reflect-metadata": "^0.1.13",
    "vue": "^2.5.22",
    "vue-axios": "^2.1.4",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.0.2",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuetify": "^1.5.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.0.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
    "pug-lint-vue": "^0.1.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.29.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb",
      "@vue/typescript",
      "plugin:import/typescript"
    ],
    "rules": {
      "camelcase": 0,
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-underscore-dangle": 0,
      "import/prefer-default-export": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "globals": {
      "COMMITHASH": true
    }
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "cache": false,
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      ".+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      ".+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.css$": "<rootDir>/tests/mocks/stub.js"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/**/*.spec.js"
    ]
  },
  "pugLintConfig": {
    "disallowAttributeConcatenation": true,
    "disallowAttributeInterpolation": true,
    "disallowClassAttributeWithStaticValue": true,
    "disallowDuplicateAttributes": true,
    "disallowHtmlText": true,
    "disallowIdAttributeWithStaticValue": true,
    "disallowIdLiterals": true,
    "disallowMultipleLineBreaks": true,
    "disallowSpacesInsideAttributeBrackets": true,
    "disallowStringConcatenation": true,
    "disallowTrailingSpaces": true,
    "maximumLineLength": 100,
    "requireClassLiteralsBeforeAttributes": true,
    "requireClassLiteralsBeforeIdLiterals": true,
    "requireIdLiteralsBeforeAttributes": true,
    "requireLineFeedAtFileEnd": true,
    "requireLowerCaseAttributes": true,
    "requireLowerCaseTags": true,
    "requireSpaceAfterCodeOperator": true,
    "requireStrictEqualityOperators": true,
    "validateAttributeQuoteMarks": "\"",
    "validateAttributeSeparator": {
      "separator": ", ",
      "multiLineSeparator": ",\n    "
    },
    "validateDivTags": true,
    "validateExtensions": true,
    "validateIndentation": 2,
    "validateLineBreaks": "LF",
    "validateSelfClosingTags": true,
    "validateTemplateString": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
